{
  "name": "angular2-webpack",
  "version": "0.0.2",
  "description": "A webpack starter for angular 2",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --progress --colors --inline --hot",
    "build": "npm version patch && rimraf build && webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "serve:build": "http-server ./build --cors -c-1 -o"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.13.1",
    "history": "^3.0.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-promise": "^0.5.3",
    "core-js": "^2.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.3.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "lost": "^7.0.3",
    "postcss-at2x": "^2.0.0",
    "postcss-calc": "^5.3.0",
    "postcss-image-sizes": "0.0.5",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
